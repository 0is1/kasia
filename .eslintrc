{
  "extends": "airbnb-base",
  "globals": {
    "fetch": false
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "__repress"
        ]
      }
    ]
  },
  "env": {
    "jest": true
  }
}
