{
  "name": "repress",
  "version": "0.0.1",
  "description": "A React Redux Toolset for the WordPress API",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.outlandish.com:repress/repress.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "humps": "^1.0.0",
    "jest-cli": "^11.0.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-saga": "^0.9.5"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.2",
    "babel-jest": "^11.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.1",
    "eslint-plugin-import": "^1.5.0",
    "react-addons-test-utils": "^15.0.1",
    "sinon": "^1.17.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/enzyme"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ]
  }
}
