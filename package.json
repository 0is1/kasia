{
  "name": "pepperoni",
  "version": "0.0.1",
  "description": "A React Redux Toolset for the WordPress API",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "compile": "rimraf lib && babel -d lib/ src/",
    "lint": "eslint src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "npm run -s compile"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.outlandish.com:pepperoni/pepperoni.git"
  },
  "keywords": [
    "javascript",
    "react",
    "redux",
    "wordpress",
    "word press",
    "wp-api",
    "wordpress api"
  ],
  "author": "Outlandish <hello@outlandish.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/outlandishideas/pepperoni/issues"
  },
  "homepage": "https://github.com/outlandishideas/pepperoni#readme",
  "dependencies": {
    "humps": "^1.0.0",
    "invariant": "^2.2.1",
    "lodash.clonedeep": "^4.1.0",
    "lodash.find": "^4.3.0",
    "lodash.merge": "^4.3.5",
    "normalizr": "^2.0.1",
    "urlencode": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-saga": "^0.9.5"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.2",
    "babel-jest": "^11.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.1",
    "eslint-plugin-import": "^1.5.0",
    "jest": "^0.1.40",
    "jest-cli": "^11.0.2",
    "rimraf": "^2.5.2"
  },
  "jest": {
    "testDirectoryName": [
      "test"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/lodash"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "/components/",
      "/util/"
    ]
  }
}
